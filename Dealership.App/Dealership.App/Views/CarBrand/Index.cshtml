@model IEnumerable<Dealership.App.Models.CarBrand.CarBrandViewModel>

<div class="w3-panel">
    <div class="w3-row-padding" style="margin:0 -16px">
        <div class="w3-twothird">
            <h5>Feeds</h5>
            @if (Model.Count() > 0)
            {
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th scope="col">Car brands</th>
                            <th scope="col">Edit</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var brand in Model)
                        {
                        <tr>
                            <td scope="row">@brand.CarBrandName</td>
                            <td><a asp-action="Update" asp-controller="CarBrand" asp-route-Id="@brand.CarBrandId" class="btn btn-primary mx1">Edit</a></td>
                            <td><a asp-action="Delete" asp-controller="CarBrand" asp-route-Id="@brand.CarBrandId" class="btn btn-danger mx1">Delete</a></td>
                        </tr>
                        }
                    </tbody>
                </table>
            }
            else
            {
            <p>No Data in Db</p>
            }
        </div>
    </div>
</div>
<hr>