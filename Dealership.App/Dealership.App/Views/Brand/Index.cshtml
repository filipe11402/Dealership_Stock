@model IEnumerable<Dealership.App.Models.BrandViewModel>

<div class="w3-panel">
    <div class="w3-row-padding" style="margin:0 -16px">
        <div class="w3-twothird">
            <h5>Feeds</h5>
            @if (Model.Count() > 0)
            {
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">Car brand</th>
                        </tr>
                    </thead>
                    <tbody>
                        @for (var counter = 0; counter < Model.Count(); counter++)
                        {
                            <tr>
                                <th scope="row">@Model.ToList()[counter].CarBrandName</th>
                            </tr>
                        }
                    </tbody>
                </table>
            }
            <p>No Data in Db</p>
        </div>
    </div>
</div>
<hr>