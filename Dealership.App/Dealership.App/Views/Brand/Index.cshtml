@model IEnumerable<Dealership.App.Models.BrandViewModel>

<div class="w3-panel">
    <div class="w3-row-padding" style="margin:0 -16px">
        <div class="w3-twothird">
            <h5>Feeds</h5>
            @if (Model.Count() > 0)
            {
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th scope="col">Car brands</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach(var brand in Model)
                        {
                            <tr>
                                <td scope="row">@brand.CarBrandName</td>
                            </tr>
                        }
                    </tbody>
                </table>
            }
            else
            {
            <p>No Data in Db</p>
            }
        </div>
    </div>
</div>
<hr>